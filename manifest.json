{
    "id": "ps-command-palette",
    "name": "Ps Command Palette",
    "version": "0.1.0",
    "author": "Josh Duncan",
    "main": "index.html",
    "host": [
        {
            "app": "PS",
            "minVersion": "23.0.0",
            "data": {
                "apiVersion": 2,
                "loadEvent": "use",
                "enableMenuRecording": true
            }
        }
    ],
    "manifestVersion": 5,
    "requiredPermissions": {
        "localFileSystem": "plugin"
    },
    "entrypoints": [
        {
            "type": "command",
            "id": "launchPalette",
            "label": {
                "default": "Ps Command Palette"
            }
        }
    ],
    "icons": [
        {
            "width": 23,
            "height": 23,
            "path": "icons/dark.png",
            "scale": [1, 2],
            "theme": ["darkest", "dark", "medium"]
        },
        {
            "width": 23,
            "height": 23,
            "path": "icons/light.png",
            "scale": [1, 2],
            "theme": ["lightest", "light"]
        }
    ]
}
